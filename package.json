{
  "name": "robodashboard",
  "version": "1.0.0",
  "description": "Web dashboard for displaying data from and controlling teleoperated robots",
  "main": "src/main.js",
  "preferGlobal": true,
  "bin": {
    "robodashboard": "src/main.js"
  },
  "scripts": {
    "client": "cd client && react-scripts start",
    "client-build": "cd client && react-scripts build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "dev-server": "cd client && react-scripts build && cd .. && npm run server",
    "server": "node src/main.js",
    "test": "ava -v"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackw01/robodashboard.git"
  },
  "keywords": [
    "robot"
  ],
  "author": "jackw01",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackw01/robodashboard/issues"
  },
  "homepage": "https://github.com/jackw01/robodashboard#readme",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "express": "^4.16.4",
    "react": "^16.4.0",
    "react-dom": "^16.6.3",
    "react-vis": "^1.11.4",
    "reactstrap": "^6.5.0",
    "serialport": "^7.0.2",
    "winston": "^3.1.0",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "ava": "1.0.0-rc.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "react-scripts": "^2.1.1"
  }
}
