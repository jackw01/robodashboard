{
  "name": "robodashboard",
  "version": "1.0.0",
  "date": "2018-12-21",
  "description": "Web dashboard for displaying data from and controlling teleoperated robots",
  "main": "src/main.js",
  "preferGlobal": true,
  "bin": {
    "robodashboard": "src/main.js"
  },
  "scripts": {
    "client": "cd client && react-scripts start",
    "client-build": "cd client && react-scripts build",
    "dev-rb": "concurrently --kill-others-on-fail \"npm run server-robot-bridge\" \"npm run client\"",
    "dev-frc": "concurrently --kill-others-on-fail \"npm run server-frc\" \"npm run client\"",
    "dev-frc-server": "cd examples/frc/java/ && javac Test.java && java Test && cd ../../.. ",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "server-robot-bridge": "node examples/robot-bridge/main.js",
    "server-frc": "node examples/frc/main.js",
    "test": "ava -v"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackw01/robodashboard.git"
  },
  "keywords": [
    "robot"
  ],
  "author": "jackw01",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackw01/robodashboard/issues"
  },
  "homepage": "https://github.com/jackw01/robodashboard#readme",
  "dependencies": {
    "anser": "^1.4.7",
    "bootstrap": "^4.1.3",
    "dateformat": "^3.0.3",
    "escape-carriage": "^1.3.0",
    "express": "^4.16.4",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dom": "^16.6.3",
    "react-icons": "^3.2.2",
    "react-resize-aware": "^2.7.1",
    "react-sortable-hoc": "^0.8.3",
    "react-vis": "^1.11.4",
    "reactstrap": "^6.5.0",
    "serialport": "^7.0.2",
    "strip-ansi": "^5.0.0",
    "systeminformation": "^3.48.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.1.0",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "ava": "1.0.0-rc.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "react-scripts": "^2.1.1"
  }
}
